{
  "name": "bananas",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p <PACKAGE_NAME>",
    "buildDev": "pesy",
    "release": { "releasedBinaries": ["BananasApp.exe"] }
  },
  "buildDirs": {
    "test": {
      "imports": ["Rely = require('@reason-native/rely/lib')"],
      "require": ["bananas.lib", "console.lib", "rely.lib"],
      "main": "TestBananas",
      "name": "TestBananas.exe"
    },
    "library": {
      "imports": ["Console = require('@reason-native/console/lib')"],
      "require": ["console.lib"],
      "name": "bananas.lib",
      "namespace": "Bananas"
    },
    "executable": {
      "imports": ["Console = require('@reason-native/console/lib')"],
      "require": ["console.lib", "bananas.lib"],
      "main": "BananasApp",
      "name": "BananasApp.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x TestBananas.exe"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/batteries": "2.10.0",
    "@opam/core": "v0.12.4",
    "@opam/dune": ">=1.6.0",
    "@opam/rtop": "3.5.2",
    "@opam/utop": "2.4.2",
    "@reason-native/console": "^0.1.0",
    "@reason-native/rely": "^3.1.0",
    "ocaml": "^4.4.0",
    "rtop": "^0.4.6",
    "utop": "^1.4.1"
  },
  "devDependencies": {
    "ocaml": "4.7.1004",
    "refmterr": "*",
    "pesy": "*",
    "@opam/merlin": "*"
  }
}
